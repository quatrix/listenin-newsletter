<table border="0" cellpadding="0" cellspacing="0">
  <thead>
  <tr><td>Track Name</td><td>Count</td></tr>
  </thead>
  {{#eachData this}}
  {{#everyOther index 2}}
  <tr>
  {{else}}
  <tr class='genres'>
  {{/everyOther}}
    <td>{{this.artist}} - {{this.title}}</td>
    <td class='align-center'>{{this.count}}</td>
  </tr>
  {{/eachData}}
</table>
