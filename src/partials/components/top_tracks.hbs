<table border="0" cellpadding="0" cellspacing="0">
  <thead>
    <tr><td class='bold'>Track</td><td class='bold'>Played</td></tr>
  </thead>
  {{#each this}}
      <tr><td colspan=2><hr/></td></tr>
      <tr>
        <td>&nbsp;<i>{{this.artist}} - {{this.title}}</i></td>
        <td class='align-center'>{{this.count}}</td>
      </tr>
  {{/each}}
</table>
