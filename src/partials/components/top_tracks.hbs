<table border="0" cellpadding="0" cellspacing="0">
  <thead>
    <tr><td>Track</td><td>Played</td></tr>
  </thead>
  {{#eachData this}}
      <tr><td colspan=2><hr/></td></tr>
  <tr class='genres'>
    <td>&nbsp;<b>{{this.artist}}</b>, <i> {{this.title}}</i></td>
    <td class='align-center'>{{this.count}}</td>
  </tr>
  {{/eachData}}
</table>
